{
  "function": {
    "taskTodoLF": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "todosLambda": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "todosTable",
          "attributes": [
            "Name",
            "Arn"
          ],
          "attributeEnvMap": {
            "Name": "TABLE_NAME",
            "Arn": "TABLE_ARN"
          }
        }
      ]
    }
  },
  "api": {
    "todosApi": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "todosLambda",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  },
  "storage": {
    "todosTable": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    }
  }
}